[FORMAT]
max-line-length=120

[BASIC]
# Naming style matching correct argument names.
argument-naming-style=camelCase

# Naming style matching correct variable names. snake_case
variable-naming-style=camelCase

# Naming style matching correct method names. snake_case
method-naming-style=camelCase

# Naming style matching correct function names. snake_case
function-naming-style=camelCase

# Naming style matching correct constant names. UPPER_CASE
const-naming-style=camelCase